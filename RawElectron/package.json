{
  "name": "ebsoc-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "electron-builder --windows nsis --publish never",
    "deploy": "electron-builder --windows nsis --publish always",
    "publish": "electron-builder --windows nsis --publish always",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "ebsoc": "file:../ebsoc",
    "ebsoc-lib": "^1.0.0",
    "electron-updater": "^4.3.9",
    "jquery": "^3.6.0",
    "puppeteer": "^10.1.0"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "appId": "com.ebsoc.app",
    "productName": "ebsoc-app",
    "copyright": "Copyright Â© 2021 kewool, rmagur1203",
    "protocols": {
      "name": "ebsoc-app",
      "schemes": [
        "ebsoc"
      ]
    },
    "mac": {
      "category": "ebs.online.class.electron.application"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "kewool",
        "repo": "ebs_desktop_app",
        "token": "%token%"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/",
      "app": "."
    }
  }
}