{
  "name": "ebsoc-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "npm run build:linux && npm run build:win",
    "build:osx": "electron-builder --mac",
    "build:linux": "npm run build:linux32 && npm run build:linux64",
    "build:linux32": "electron-builder --linux --ia32",
    "build:linux64": "electron-builder --linux --x64",
    "build:win": "npm run build:win32 && npm run build:win64",
    "build:win32": "electron-builder --win nsis --ia32",
    "build:win64": "electron-builder --win nsis --x64"
  },
  "keywords": [],
  "author": "kewool <crobrine@naver.com> (https://kewool.net/)",
  "contributors": [
    {
      "name": "kewool",
      "email": "crobrine@naver.com"
    },
    {
      "name": "rmagur1203",
      "email": "rmagur1203@naver.com"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@ffprobe-installer/ffprobe": "^1.1.0",
    "axios": "^0.21.1",
    "ebsoc": "file:ebsoc",
    "ebsoc-lib": "^1.0.0",
    "electron-updater": "^4.3.9",
    "jquery": "^3.6.0",
    "puppeteer": "^10.1.0"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "appId": "com.ebsoc.app",
    "productName": "ebsoc-app",
    "copyright": "Copyright Â© 2021 kewool, rmagur1203",
    "protocols": {
      "name": "ebsoc-app",
      "schemes": [
        "ebsoc"
      ]
    },
    "mac": {
      "category": "ebs.online.class.electron.application",
      "target": [
        "default"
      ],
      "icon": "./resources/installer/icon.icns"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "./resources/installer/icon.png"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "kewool",
        "repo": "ebs_desktop_app"
      }
    ],
    "dmg": {
      "title": "ebsoc-app",
      "icon": "./resources/installer/icon.icns"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/",
      "app": "."
    }
  }
}
